{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Template mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/YummyLoop/fabric-mod-template",
    "sources": "https://github.com/YummyLoop/fabric-mod-template"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "yummyloop.template.TemplateMod"
      }
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabric-language-kotlin": ">=1.3.30",
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
